    #fh5co-wrapper
      #fh5co-page
        #fh5co-header
          header#fh5co-header-section
            .container.MyContainer
              .nav-header
                a.js-fh5co-nav-toggle.fh5co-nav-toggle(href='#')
                h1#fh5co-logo
                  a(href='/')
                    .imgContainer
                      img(src="images/photos/lol.png", alt="" width="250px" height="150px")
                    p.whiteSmall Prévoyance Avocat.
                // START #fh5co-menu-wrap
                nav#fh5co-menu-wrap(role='navigation')
                  ul#fh5co-primary-menu.sf-menu
                    li.active
                      a(href='/') Home
                    li
